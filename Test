
local RandomLib = {}

function RandomLib.new(Name)

local Random = Instance.new("ScreenGui")
local Drag = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local Frame = Instance.new("Frame")
local ScrollingFrame = Instance.new("ScrollingFrame")
local UIListLayout = Instance.new("UIListLayout")
local UICorner_2 = Instance.new("UICorner")
local UICorner_3 = Instance.new("UICorner")

--Properties:

Random.Name = Name
Random.Parent = game.CoreGui

Drag.Name = "Drag"
Drag.Parent = Random
Drag.Active = true
Drag.BackgroundColor3 = Color3.fromRGB(68, 68, 68)
Drag.Position = UDim2.new(0.366933137, 0, 0.173605576, 0)
	Drag.Size = UDim2.new(0, 320, 0, 26)
	Drag.Draggable = true

UICorner.Parent = Drag

Frame.Parent = Drag
Frame.Active = true
Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Frame.Position = UDim2.new(-0.000685095787, 0, 0.998594284, 0)
Frame.Size = UDim2.new(0, 320, 0, 352)

ScrollingFrame.Parent = Frame
ScrollingFrame.Active = true
ScrollingFrame.BackgroundColor3 = Color3.fromRGB(41, 41, 41)
ScrollingFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
ScrollingFrame.BorderSizePixel = 0
ScrollingFrame.Position = UDim2.new(0.0562499985, 0, 0.0255681816, 0)
ScrollingFrame.Size = UDim2.new(0, 288, 0, 328)
ScrollingFrame.CanvasSize = UDim2.new(0, 0, 5, 0)
ScrollingFrame.ScrollBarThickness = 6
ScrollingFrame.VerticalScrollBarPosition = Enum.VerticalScrollBarPosition.Left

UIListLayout.Parent = ScrollingFrame
UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout.Padding = UDim.new(10, 10)

    UICorner_2.Parent = ScrollingFrame

	UICorner_3.Parent = Frame
	
	return RandomLib
end

function RandomLib.ButtonNew(Name)
	local Template = Instance.new("TextButton")
	local UICorner = Instance.new("UICorner")

	Template.Name = Name
	Template.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
	Template.Position = UDim2.new(0.0642361119, 0, 0, 0)
	Template.Size = UDim2.new(0, 267, 0, 23)
	Template.Font = Enum.Font.Unknown
	Template.TextColor3 = Color3.fromRGB(255, 255, 255)
	Template.TextSize = 15.000
	Template.TextStrokeColor3 = Color3.fromRGB(50, 50, 50)
	Template.TextStrokeTransparency = 0.500
	Template.TextXAlignment = Enum.TextXAlignment.Left

	UICorner.Parent = Template
	
	return Template
	
end

return RandomLib
